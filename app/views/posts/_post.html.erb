<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
    <%= render partial: 'likes/button', locals: { likeable: post } %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Description_first:</strong>
    <%= post.description_first %>
  </p>
</div>
